{
    "manifest_version": 3,
    "name": "Ejobs Navbar Fixer",
    "version": "1.0",
    "description": "It fixes the navbar",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["*://www.ejobs.ro/*"],
        "css": ["styles.css"]
      }
    ]
  }
  